meta {
  name: Signup
  type: http
  seq: 1
}

post {
  url: {{baseUrl}}/v1/auth/signup
  body: json
  auth: none
}

body:json {
  {
    "name": "Test User",
    "email": "test@example.com",
    "password": "password123"
  }
}

script:post-response {
  if (res.status === 201) {
    bru.setEnvVar("token", res.body.data.token);
  }
}
